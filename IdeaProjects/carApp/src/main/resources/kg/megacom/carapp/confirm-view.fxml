<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="108.0" prefWidth="371.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kg.megacom.carapp.controllers.ConfirmController">
   <children>
      <Label fx:id="lblConfirm" layoutX="14.0" layoutY="14.0" prefHeight="28.0" prefWidth="328.0" text="Вы уверены в том, что хотите удалить?">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <ButtonBar layoutX="157.0" layoutY="54.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button defaultButton="true" mnemonicParsing="false" onAction="#onYes" text="Да" />
            <Button cancelButton="true" mnemonicParsing="false" onAction="#onCancel" text="Отмена" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
